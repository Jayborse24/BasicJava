package com.practiceproblems;

public class SmallestElement {
	public static void main(String[] args) {
		int arr[]= {1,2,3,4,5};
		System.out.println("Smallest Element of Array is: ");
		int Smallest=arr[0];
		for(int i=0; i<arr.length; i++) {
			if(Smallest>arr[i]) {
				Smallest=arr[i];
			}
		}
		System.out.println(Smallest);
	}

}

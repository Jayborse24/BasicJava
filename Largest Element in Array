package com.practiceproblems;

public class LargestElement {
	public static void main(String[] args) {
		int arr[]= {1,2,3,4,5};
		System.out.println("Largest Element of Array is: ");
		int Largest=arr[0];
		for(int i=0; i<arr.length; i++) {
			if(Largest<arr[i]) {
				Largest=arr[i];
			}
		}
		System.out.println(Largest);
	}

}
